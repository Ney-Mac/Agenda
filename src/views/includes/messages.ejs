<% if(errors.length > 0){ %>

    <div class="row">
        <div class="col my-3">
            <div class="alert alert-danger">

                <% errors.forEach(erro =>{ %>
                    <%= erro %> <br>
                <% }); %>
            
            </div>
        </div>
    </div>

<% }else if(success != ''){ %>

    <div class="row">
        <div class="col my-3">
            <div class="alert alert-success">

                <%= success %> <br>
                
            </div>
        </div>
    </div>

<% } %>